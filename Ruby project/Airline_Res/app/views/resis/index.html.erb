<p id="notice"><%= notice %></p>

<h1>Resis</h1>

<table>
  <thead>
    <tr>
      <th>Fisrt name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Password</th>
      <th>Confirm password</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resis.each do |resi| %>
      <tr>
        <td><%= resi.Fisrt_Name %></td>
        <td><%= resi.Last_Name %></td>
        <td><%= resi.Email %></td>
        <td><%= resi.Password %></td>
        <td><%= resi.Confirm_Password %></td>
        <td><%= link_to 'Show', resi %></td>
        <td><%= link_to 'Edit', edit_resi_path(resi) %></td>
        <td><%= link_to 'Destroy', resi, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resi', new_resi_path %>
